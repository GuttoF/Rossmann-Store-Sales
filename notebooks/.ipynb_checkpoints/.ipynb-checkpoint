    "## 1.6 Fillout NA"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {
    "ExecuteTime": {
     "end_time": "2021-01-15T17:58:43.778357Z",
     "start_time": "2021-01-15T17:58:43.770376Z"
    },
    "hidden": true
   },
